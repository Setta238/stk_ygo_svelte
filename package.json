{
  "name": "stk_ygo_svelte",
  "homepage": "https://github.com/Setta238/stk_ygo_svelte",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "trim-json":"powershell.exe -NoProfile -ExecutionPolicy Unrestricted -file ./bat/TrimJson.ps1 ./docs/json",
    "dist-to-docs":"robocopy dist docs /MIR /DCOPY:DAT | exit 0",
    "predeploy": "npm run check && vite build && npm run dist-to-docs && npm run trim-json",
    "deploy": "gh-pages -d docs",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "prettier": "^3.5.1",
    "rollup": "^4.34.7",
    "rollup-plugin-visualizer": "^5.14.0",
    "svelte": "^5.19.6",
    "svelte-check": "^4.1.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.1.0",
    "vite-plugin-checker": "^0.9.1"
  }
}
